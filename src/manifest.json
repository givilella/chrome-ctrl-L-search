{
  "name": "Search Field Autofocus",
  "version": "2.0",
  "manifest_version": 3,
  "description": "Automatically focuses on the search field of a website",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["tabs"],
  "host_permissions": ["<all_urls>"],
  "action": {},
  "icons": {}
}
